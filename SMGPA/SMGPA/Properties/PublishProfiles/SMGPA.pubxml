<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://localhost/SMGPA</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>localhost</MSDeployServiceURL>
    <DeployIisAppPath>Default Web Site/SMGPA</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>InProc</MSDeployPublishMethod>
    <EnableMSDeployBackup>False</EnableMSDeployBackup>
    <UserName />
    <_SavePWD>False</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="SMGPAConnection" Order="1" Enabled="False">
          <Destination Path="Data Source=EMILIO-PC\POPE;Initial Catalog=SMGPA_SCHEMA;User ID=sa;Password=123.pass" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="SMGPA.Models.SMGPAContext, SMGPA" MigrationConfiguration="SMGPA.Migrations.Configuration, SMGPA" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)SMGPAConnection-Web.config Connection String">
      <ParameterValue>Data Source=EMILIO-PC\POPE;Initial Catalog=SMGPA_SCHEMA;User ID=sa;Password=123.pass</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>